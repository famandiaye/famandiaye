package exercice71;

public class Compte {
  int numero;
  String typeCompte;
  double solde;
  int codeSecret;
  //constructeur par defaut:
  
public Compte() {
   this.numero = 999999;
	this.typeCompte = '';
	this.solde = 0;
	thiscodeSecret = 0;
	}
//methode:

 public void attribuerCodeSecret() {
	 int codeSecret = (int)(math.random*9899+100)
 }
 
 //constructeur sur charge:
 
 public Compte(int numero, String typeCompte, double solde) {
	    this.numero = numero;
	    if (typeCompte.equals("D") || typeCompte.equals("E")) {
	        this.typeCompte = typeCompte;
	    } else {
	        this.typeCompte = ";
	    }
	    this.solde = solde;
	    this.coteSecret = 0;
 }
//modificateurs seteurs:
	    

public void setNumero(int numero) {
    this.numero = numero;
}

public void setTypeCompte(String typeCompte) {
    this.typeCompte = typeCompte;
}

//accesseurs geteurs:


public void setNumero(int numero) {
    this.numero = numero;
}

public void setTypeCompte(String typeCompte) {
    this.typeCompte = typeCompte;
}


}
